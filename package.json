  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "build": "tsc",
    "prebuild": "npm run clean",
    "start:playground": "node -r dotenv/config ./build/index.js",
    "start:prod": "NODE_ENV=production node -r dotenv/config ./build/index.js",
    "start": "NODE_ENV=staging node -r dotenv/config ./build/index.js",
    "dev:build": "tsc -w --preserveWatchOutput",
    "dev:run": "NODE_ENV=development nodemon --watch './build' --enable-source-maps -r dotenv/config ./build/index.js",
    "dev": "npm run build && concurrently -c 'auto' --kill-others npm:dev:*"
  },
