{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "outDir": "build",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": [
      "DOM"
    ]
  },
  "include": [
    "typings/**/*.ts",
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "exclude": [
    "**/*.spec.ts"
  ]
}